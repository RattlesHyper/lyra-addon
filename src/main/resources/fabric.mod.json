{
  "schemaVersion": 1,
  "id": "lyra-addon",
  "version": "${version}",
  "name": "Lyra Addon",
  "description": "An addon to improve creative mode experience for Meteor Client",
  "authors": [
    "RattlesHyper",
    "gasps"
  ],
  "contact": {
    "repo": "https://github.com/RattlesHyper/lyra-addon",
    "issues": "https://github.com/RattlesHyper/lyra-addon/issues",
    "discord": "https://discord.com/invite/uFpaAZdVgS"
  },
  "icon": "assets/lyra/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.lyra.addon.Addon"
    ]
  },
  "mixins": [
    "lyra.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
